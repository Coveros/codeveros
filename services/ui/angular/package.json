{
  "name": "@codeveros/ui",
  "version": "2.0.0",
  "description": "Codeveros Angular UI",
  "private": true,
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider ng serve --host 0.0.0.0",
    "start.proxy": "cross-env NODE_OPTIONS=--openssl-legacy-provider ng serve --host 0.0.0.0 --proxy-config proxy.json",
    "test": "ng test --code-coverage",
    "test-ci": "npm run test",
    "checkstyle": "ng lint",
    "checkstyle:ci": "npm run checkstyle",
    "e2e": "ng e2e",
    "build": "cross-env NODE_OPTIONS=--openssl-legacy-provider ng build",
    "build.production": "cross-env NODE_OPTIONS=--openssl-legacy-provider ng build -c production"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com:Coveros/codeveros.git"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/animations": "^17.3.12",
    "@angular/cdk": "^17.3.10",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/flex-layout": "^14.0.0-beta.41",
    "@angular/forms": "^17.3.12",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/router": "^17.3.12",
    "@auth0/angular-jwt": "^5.0.2",
    "rxjs": "^7.3.0",
    "swagger-ui": "^4.1.3",
    "tslib": "^2.3.1",
    "zone.js": "^0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.17",
    "@angular-devkit/core": "^17.3.17",
    "@angular-eslint/builder": "^17.5.3",
    "@angular-eslint/eslint-plugin": "^17.5.3",
    "@angular-eslint/eslint-plugin-template": "^17.5.3",
    "@angular-eslint/schematics": "^17.5.3",
    "@angular-eslint/template-parser": "^17.5.3",
    "@angular/cli": "^17.3.17",
    "@angular/compiler-cli": "^17.3.12",
    "@angular/language-service": "^17.3.12",
    "@types/jasmine": "^3.8.2",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^15.14.7",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "cross-env": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsdoc": "^36.0.7",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jasmine-core": "^3.8.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-junit-reporter": "^2.0.1",
    "ng-packagr": "^17.3.0",
    "protractor": "^7.0.0",
    "puppeteer": "^10.2.0",
    "ts-node": "^10.2.0",
    "typescript": "^5.8.3"
  }
}
